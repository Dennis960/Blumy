	"scripts": {
		"dev": "vite dev --host",
		"build": "vite build",
		"preview": "vite preview --host",
		"test": "POSTGRES_DB=blumy_test npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:integration": "POSTGRES_DB=blumy_test playwright test",
		"test:ui": "POSTGRES_DB=blumy_test playwright test --ui",
		"test:unit": "vitest",
		"lint": "eslint .",
		"format": "prettier --write src",
		"drizzle": "drizzle-kit generate"
	},
