{
  "compilerOptions": {
    "module": "ES2022",
    "target": "ES2022",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist",

    "strict": true,
    "skipLibCheck": true
  },
  "include": ["**/*.d.ts", "**/*.js", "**/*.ts"]
}
